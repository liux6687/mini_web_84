page {
  background: #313132;
}
.login {
  color: #fff;
}
.login .top {
  margin-top: 168rpx;
  text-align: center;
  margin-bottom: 150rpx;
}
.login .top .logo {
  width: 140rpx;
  height: 140rpx;
  background: #fff;
  margin: 0 auto;
  margin-bottom: 40rpx;
  border-radius: 30rpx;
  overflow: hidden;
}
.login .top .h1 {
  font-size: 30rpx;
  margin-bottom: 24rpx;
}
.login .top .h2 {
  color: #9b9b9b;
}

.login .bottom {
  width: 358rpx;
  margin: 0 auto;
}
.login .bottom .icon {
  width: 17rpx;
  height: 17rpx;
  margin-left: 5rpx;
}
.login .bottom .icon-yes {
  width: 20rpx;
  height: 14rpx;
  margin-left: 10rpx;
}
.login .bottom .phone, .login .bottom .name {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
}

.login .bottom .phone {
  margin-bottom: 50rpx;
}
.login .bottom .move {
  color: #ef6f3e;
  display: flex;
  align-items: center;
  position: relative;
}
.getUserInfo {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
}



/* 获取手机号 */

.phoneBox {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.6);
    display: flex;
    justify-content: center;
    align-items: center;
}

.phoneBox .content {
    width: 560rpx;
    height: 564rpx;
    background-color: #fff;
    text-align: center;
    padding: 0 90rpx;
    box-sizing: border-box;
    border-radius: 12rpx;
}

.phoneBox .content .title {
    font-size: 34rpx;
    color: #2c2c2c;
    margin-top: 65rpx;
}

.phoneBox .content .text {
    text-align: left;
    font-size: 26rpx;
    color: #888;
    margin-top: 60rpx;
}

.phoneBox .content button {
    width: 100%;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    font-size: 30rpx;
    color: #fff;
    background-color: #19ab18;
    border-radius: 60rpx;
    margin-top: 85rpx;
}

.phoneBox .content .phoneOther {
    width: 100%;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    font-size: 30rpx;
    color: #fff;
    background-color: #19ab18;
    border-radius: 60rpx;
    margin-top: 40rpx;
}
.model {
  color: #000;
}
.model .model-content .items {
  height: 80rpx;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
}
.model .model-content .items image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 25rpx;
}