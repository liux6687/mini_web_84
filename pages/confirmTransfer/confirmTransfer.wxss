.header {
   padding: 24rpx;
   background: #fff;
   display: flex;
   border-bottom: 2rpx solid #f5f4f9;
}
.header .img-box {
  flex: 0 0 160rpx;
  height: 160rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-right: 30rpx;
}
.right {
  padding-top: 15rpx;
}
.right .h1 {
  font-weight: bold;
  line-height:  40rpx;
  margin-bottom: 25rpx;
}
.right .Id text:first-child {
  padding: 11rpx 15rpx;
  border-radius: 5rpx;
  font-size: 20rpx;
  background: rgba(0,0,0,.1);
  margin-right: 30rpx;
}
/* content */
.content {
  padding: 18rpx;
}
.content .relus {
  padding: 26rpx;
  background: #fff;
  border-radius: 10rpx;
  line-height: 50rpx;
  color: #ef6f3e;
  margin-bottom: 18rpx;
}
.content .relus .li {
  display: flex;
  align-items: center;
}
.content .relus .img-box {
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}
/* income */
.income {
  padding: 30rpx 26rpx 0;
  border-radius: 10rpx;
  background: #fff;
}
.income .title {
  padding: 0 14rpx;
}
.income .top .h1 {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 30rpx 0;
  box-sizing: border-box; 
  border-bottom: 2rpx dashed #f5f4f9;
} 
.income .top .h1 view {
  padding: 0 14rpx;
}
.income .top .h1 .money {
  border-right: 2rpx solid #f5f4f9;
}
.income .top .h1 .money text {
  font-size: 35rpx;
}
.income .top .h1 .zf text {
  color: #ef6f3e;
}

/* main */
.income .main {
  padding: 30rpx 14rpx;
  display: flex;
}
.income .main .main-left {
  margin-right: 40rpx;
}
.income .main .main-title {
  color: #989dae;
  font-size: 20rpx;
}
.income .main .main-num {
  font-weight: bold;
  font-size: 26rpx;
  padding: 16rpx 0 30rpx;
}
/* bottom */
.income .bottom {
  height: 100rpx;
  padding: 0 14rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-weight: bold;
}
.income .bottom .h1 {
  color: #989dae;
}
.income .bottom .result {
  color: #ef6f3e;
}
.income .bottom .result text {
  font-size: 20rpx;
}


 .submit-box {
  padding: 20rpx;
  background: #fff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}
.submit-box view {
  width: 100%;
  height: 96rpx;
  background: #2c2c2c;
  color: #fff;
  line-height: 96rpx;
  text-align: center;
  font-weight: bold;
  font-size: 30rpx;
  border-radius: 10rpx;
}